{
    "name": "cheetahvault",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "cd frontend && npx vue-cli-service build",
        "start": "cd server && node index.js",
        
        "startpm2": "pm2 start pm2.config.js --env production",
        "restartpm2": "pm2 restart pm2.config.js --env production",
        "stoppm2": "pm2 stop pm2.config.js --env production",

        "install": "cd frontend && npm install && cd ../server && npm install"
    },
